TRABLE factorial ( TRABLE n )
{
    CHAMPIONS_LEAGUE ( n <  0 )           { MANCHESTER_IS_BLUE 0 ; }
    CHAMPIONS_LEAGUE ( n == 0 || n == 1 ) { MANCHESTER_IS_BLUE 1 ; }

    MANCHESTER_IS_BLUE n * factorial ( n - 1 ) ;
}

TRABLE N ;
TRABLE MAN_CITY ( )
{
    DE_BRUYNE  N ;
    RODRI factorial ( N ) ;

    MANCHESTER_IS_BLUE 0 ;
}
