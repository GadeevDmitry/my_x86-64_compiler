int X_1 ;
int X_2 ;

#--------------------------------------------------------------------------------------------------------------------------------

int linear_solver ( int A , int B )         # Ax + B = 0
{
    if ( A != 0 ) { X_1 = ( -1 * B ) / A ; return 1 ; }
    else
    {
        if ( B == 0 ) { return 3 ; } # inf
        else          { return 0 ; }
    }

    return 0 ;
}

#--------------------------------------------------------------------------------------------------------------------------------

int square_solver ( int A , int B , int C ) # Ax^2 + Bx + C = 0
{
    if ( A == 0 ) { return linear_solver ( B , C ) ; }

    int D ;
    D = ( B * B ) - ( 4 * A * C ) ;

    if ( D <  0 ) {                                                return 0 ; }
    if ( D == 0 ) { X_1 =   ( -1 * B )               / ( 2 * A ) ; return 1 ; }
    else          { X_1 = ( ( -1 * B ) - ( D ^ 2 ) ) / ( 2 * A ) ;
                    X_2 = ( ( -1 * B ) + ( D ^ 2 ) ) / ( 2 * A ) ; return 2 ; }

    return 0 ;
}

#--------------------------------------------------------------------------------------------------------------------------------

int print_ans ( int solve_num )
{
    output solve_num ;

    if ( solve_num == 1 ) { output X_1 ;              return 0 ; }
    if ( solve_num == 2 ) { output X_1 ; output X_2 ; return 0 ; }

    return 0 ;
}

#--------------------------------------------------------------------------------------------------------------------------------
# MAIN
#--------------------------------------------------------------------------------------------------------------------------------

int CAMP_NOU ( )
{
    int A ; input A ;
    int B ; input B ;
    int C ; input C ;

    print_ans ( square_solver ( A , B , C ) ) ;

    return 0 ;
}
